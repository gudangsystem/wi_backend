{
  "kind": "collectionType",
  "collectionName": "promos",
  "info": {
    "singularName": "promo",
    "pluralName": "promos",
    "displayName": "promo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "promos"
    },
    "date": {
      "type": "datetime",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo-data.seo"
    }
  }
}
